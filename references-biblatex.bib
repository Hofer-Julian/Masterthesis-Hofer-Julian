	file = {Huber et al. - 2018 - Semiconductor quantum dots as an ideal source of p.pdf:C\:\\Users\\Julian\\Zotero\\storage\\WG3CHZX8\\Huber et al. - 2018 - Semiconductor quantum dots as an ideal source of p.pdf:application/pdf}
